{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "inputBlob",
      "path": "functionstorage/{name}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "type": "blob"
    }
  ]
}
