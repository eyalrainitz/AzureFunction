{
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": [
          "get",
          "post"
        ]
      },
      {
        "type": "http",
        "direction": "out",
        "name": "res"
      },
      {
        "name": "inputBlob",
        "path": "functionstorage/{name}.json",
        "connection": "AzureWebJobsStorage",
        "direction": "in",
        "type": "blob"
      }
    ]
  }