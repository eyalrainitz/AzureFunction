{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "cosmosDB",
      "name": "inputDocument",
      "direction": "in",
      "connectionStringSetting": "cosmospocselapoc_DOCUMENTDB",
      "databaseName": "SampleDB",
      "collectionName": "leases",
      "createLeaseCollectionIfNotExists": false
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
